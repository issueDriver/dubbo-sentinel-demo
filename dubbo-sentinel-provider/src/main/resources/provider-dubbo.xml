<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
     	http://www.springframework.org/schema/beans/spring-beans.xsd
       	http://code.alibabatech.com/schema/dubbo
     	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--应用配置-->
    <dubbo:application name="${dubbo.application.name}">
        <dubbo:parameter key="qos.enable" value="false"/>
        <!-- <dubbo:parameter key="qos.accept.foreign.ip" value="false"/>
         <dubbo:parameter key="qos.port" value="22904"/>-->
    </dubbo:application>
    <!--注册中心-->
    <dubbo:registry protocol="${dubbo.registry.protocol}" address="${dubbo.registry.address}" timeout="${dubbo.registry.timeout}" check="false"/>
    <dubbo:consumer timeout="${dubbo.consumer.timeout}" retries="${dubbo.consumer.retries}" check="${dubbo.consumer.check}"/>


    <dubbo:reference interface="com.ydp.moviebase.api.IDemoService" id="demoServiceImpl" version="1.0"/>
    <dubbo:reference interface="com.ydp.moviebase.api.ICinemaService" id="cinemaServiceImpl" timeout="50000" version="1.0"/>
    <dubbo:reference interface="com.ydp.moviebase.api.IMovieNumberService" id="movieNumberServiceImpl" version="1.0"
                     timeout="10000"/>
    <dubbo:reference interface="com.ydp.moviebase.api.IHallService" id="hallServiceImpl" version="1.0" timeout="50000"/>
    <dubbo:reference interface="com.ydp.moviebase.api.ISeatService" id="seatServiceImpl" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IZoneService" id="zoneService" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IFilmService" id="filmService" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IMovieScheduleService" id="movieScheduleService" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IFilmResourcesService" id="filmResourcesService" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.ICinemaChainService" id="cinemaChainServiceImpl" version="1.0"/>
    <dubbo:reference interface="com.ydp.moviebase.api.ICinemaInverstorService" id="cinemaInverstorServiceImpl"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IMovieNumberScheduleService" id="movieNumberScheduleServiceImpl"
                     version="1.0"/>
    <!--影院调价-->
    <dubbo:reference interface="com.ydp.price.api.IPriceAdjustmentSchemeService" id="priceAdjustmentSchemeServiceImpl"
                     version="1.0"/>
    <!--促销活动-->
    <dubbo:reference interface="com.ydp.price.api.IPromotionSchemeService" id="promotionSchemeServiceImpl"
                     version="1.0"/>
    <!--排期价格-->
    <dubbo:reference interface="com.ydp.price.api.ISchedulePriceService" id="schedulePriceServiceImpl"
                     version="1.0"/>


    <!--渠道管理-->
    <dubbo:reference interface="com.ydp.moviebase.api.IChannelService" id="channelService"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IChannelCinemaService" id="channelCinemaService"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.ISelfChannelService" id="selfChannelService"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.ISelfChannelCinemaService" id="selfChannelCinemaService"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.ISelfBusinessService" id="selfBusinessService"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.ISelfChannelCategoryService" id="selfChannelCategoryService"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviechannel.api.IOrderService" id="orderService"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IDictService" id="dictService"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.ISpecialCityService" id="specialCityService"
                     version="1.0"/>
    <!--停售计划-->
    <dubbo:reference interface="com.ydp.price.api.IOffSaleSchemeService" id="offSaleSchemeServiceImpl"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IOperateLogService" id="operateLogService" timeout="1000"
                     version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.ILoginService" id="loginService"
                     retries="0" timeout="1000" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IMenuService" id="menuService"
                     retries="0" timeout="10000" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IRoleMenuService" id="roleMenuService"
                     retries="0" timeout="1000" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IRoleService" id="roleService"
                     retries="0" timeout="1000" version="1.0"/>

    <dubbo:reference interface="com.ydp.moviebase.api.IAccountService" id="acountService"
                     retries="0" timeout="1000" version="1.0"/>

    <!-- 一卡通 -->
    <!-- 卡券影院配置 -->
    <dubbo:reference id="storedValueCardConfCinemaService" interface="com.ydp.moviemarketing.api.IStoredValueCardConfCinemaService"
                     timeout="60000"/>
    <!-- 卡券影片配置 -->
    <dubbo:reference id="storedValueCardConfMovieService" interface="com.ydp.moviemarketing.api.IStoredValueCardConfMovieService"
                     timeout="60000"/>

    <!-- 卡批次管理 -->
    <dubbo:reference id="storedValueCardConfService" interface="com.ydp.moviemarketing.api.IStoredValueCardConfService"
                     timeout="60000"/>
    <!-- 卡券管理 -->
    <dubbo:reference id="storedValueCardService" interface="com.ydp.moviemarketing.api.IStoredValueCardService" timeout="120000"/>
    <!-- 卡券发行人-影院配置管理 -->
    <dubbo:reference id="oneCardIssuerCinemaService" interface="com.ydp.moviemarketing.api.IOneCardIssuerCinemaService"
                     timeout="120000"/>
    <!-- 存储卡档位管理 -->

    <dubbo:reference id="storedValueCardGradeService" interface="com.ydp.moviemarketing.api.IStoredValueCardGradeService" timeout="120000"/>

    <!--卡券影院授权管理-->
    <dubbo:reference interface="com.ydp.moviebase.api.IAuthorizeChannelService" id="authorizeChannelService" version="1.0" timeout="120000"/>

    <!-- 会员 -->
    <dubbo:reference interface="com.ydp.vip.api.IVipSystemService" id="vipSystemServiceImpl" version="1.0"
                     timeout="120000"/>

    <dubbo:reference interface="com.ydp.vip.api.IVipEntityCardService" id="vipEntityCardServiceImpl"
                     timeout="6000" version="1.0"/>
    <dubbo:reference interface="com.ydp.vip.api.IVipMakeCardService" id="vipMakeCardServiceImpl"
                     timeout="6000" version="1.0"/>

    <dubbo:reference interface="com.ydp.system.vendor.vip.api.IMtxCardService" id="mtxCardServiceImpl" version="1.0"
                     timeout="120000"/>

    <dubbo:reference interface="com.ydp.vip.api.IVipRechargeAmountService" id="vipRechargeAmountServiceImpl"
                     version="1.0" timeout="6000"/>

    <dubbo:reference interface="com.ydp.vip.api.IVipCardService" id="vipCardService" version="1.0" timeout="6000"/>

    <dubbo:reference interface="com.ydp.vip.api.IVipUserService" id="vipUserService" version="1.0" timeout="6000"/>

    <dubbo:reference interface="com.ydp.vip.api.IVipOrderService" id="vipOrderService" version="1.0" timeout="6000"/>

    <dubbo:reference interface="com.ydp.vip.api.IVipRecordService" id="vipRecordService" version="1.0" timeout="6000"/>



    <dubbo:reference interface="com.ydp.moviebase.api.IUserService" id="userService" version="1.0" timeout="6000"/>
    
    <!-- 卖品 -->
    <dubbo:reference id="goodsAdvertService" interface="com.ydp.goods.api.IGoodsAdvertService" timeout="60000" version="1.0"/>
    <dubbo:reference id="goodsCheckService" interface="com.ydp.goods.api.IGoodsCheckService" timeout="60000" version="1.0"/>
    <dubbo:reference id="goodsOrderService" interface="com.ydp.goods.api.IGoodsOrderService" timeout="60000" version="1.0"/>
    <dubbo:reference id="goodsRefundService" interface="com.ydp.goods.api.IGoodsRefundService" timeout="60000" version="1.0"/>
    <dubbo:reference id="goodsPickService" interface="com.ydp.goods.api.IGoodsPickService" timeout="60000" version="1.0"/>
    <dubbo:reference id="goodsService" interface="com.ydp.goods.api.IGoodsService" timeout="60000" version="1.0"/>
    <dubbo:reference id="goodsSortService" interface="com.ydp.goods.api.IGoodsSortService" timeout="60000" version="1.0"/>

    <!--消息中心-->
    <dubbo:reference interface="com.ydp.message.center.api.ITemplateService" id="templateService" version="1.0"/>
    <dubbo:reference interface="com.ydp.message.center.api.ISmsTaskService" id="smsTaskService" version="1.0"/>
    <dubbo:reference interface="com.ydp.message.center.api.IMessageClient" id="messgeClient" version="1.0"/>

    <!--支付中心-->
    <dubbo:reference interface="com.ydp.moviepay.api.IPayInfoService" id="payInfoService" version="1.0"/>
</beans>